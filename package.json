{
  "name": "gg-code",
  "version": "1.0.0",
  "description": "GG CODE - AI-Powered Code Editor CLI Tool",
  "main": "dist/index.js",
  "bin": {
    "agent": "./bin/agent.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "agent": "node run-agent.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "node scripts/clean.js",
    "clean:release": "node scripts/clean.js",
    "clean:dist": "node -e \"const fs=require('fs');fs.existsSync('dist')&&fs.rmSync('dist',{recursive:true,force:true});console.log('✓ dist 已清理')\"",
    "package": "node scripts/package-simple.js",
    "package:clean": "node scripts/package-simple.js && npm run clean:release"
  },
  "keywords": [
    "cli",
    "gg-code",
    "code-editor",
    "ai",
    "assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.68",
    "archiver": "^6.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
